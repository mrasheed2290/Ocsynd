			<head Imprint:bind="head">
				<title Imprint:bind="title">Test File</title>
			</head>
			<body>
				<ul class="Screen" Imprint:bind="Screen">
					<li id="Header" class="Stage" Imprint:bind="Header"></li>
					<li id="Main" class="Stage" Imprint:bind="Main" color="blue" flavor="orange">
						<div>content [@ A @] here</div>
						<div>content [@ B @] here</div>
						<div>content [@ C @] here</div>
						<div>content [@ D @] here</div>
					</li>
					<li id="Footer" class="Stage [@ B @]" Imprint:bind="Footer">
